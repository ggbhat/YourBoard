{
  "name": "Uboard",
  "description": "This extension demonstrates a 'browser action'",
  "version": "1.0",
"permissions": [
    "activeTab",
    "storage"
    
  ],

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    
  },

  "manifest_version": 2
}
